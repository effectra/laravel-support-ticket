{
  "name": "effectra/laravel-support-ticket",
  "description": "The Effectra support ticket package for laravel.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Effectra\\LaravelSupportTicket\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Mohammed Taha",
      "email": "info@mohammedtaha.net"
    }
  ],
  "require": {
    "effectra/laravel-status": "^1.0",
    "spatie/laravel-package-tools": "^1.92"
  },
  "minimum-stability": "stable",
  "require-dev": {
    "pestphp/pest": "^2.28",
    "symfony/var-dumper": "^6.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Effectra\\LaravelSupportTicket\\Providers\\LaravelSupportTicketServiceProvider"
      ]
    }
  }
}
